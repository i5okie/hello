[{sasl,[{errlog_type,error}]},
 {logger,
     [{console,
          [{format,<<"$time $metadata[$level] $message\n">>},
           {metadata,[user_id]}]}]},
 {hello,
     [{ecto_repos,['Elixir.Hello.Repo']},
      {'Elixir.HelloWeb.Endpoint',
          [{render_errors,
               [{view,'Elixir.HelloWeb.ErrorView'},
                {accepts,[<<"html">>,<<"json">>]}]},
           {pubsub,
               [{name,'Elixir.Hello.PubSub'},
                {adapter,'Elixir.Phoenix.PubSub.PG2'}]},
           {http,[{port,{system,<<"PORT">>}}]},
           {url,[{host,<<"localhost">>},{ip,{0,0,0,0}},{port,80}]},
           {cache_static_manifest,<<"priv/static/manifest.json">>},
           {server,true},
           {root,<<".">>},
           {version,<<"0.0.1">>},
           {secret_key_base,nil}]},
      {'Elixir.Hello.Repo',
          [{adapter,'Elixir.Ecto.Adapters.Postgres'},
           {url,nil},
           {pool_size,10},
           {ssl,true}]}]}].
